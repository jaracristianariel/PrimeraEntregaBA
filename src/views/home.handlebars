<div>
    <h1>Hola coderrrr</h1>

    <h3>lista de productos</h3>

    <table>
        <thead>
            <tr>
                <th>id</th>
                <th>title</th> 
                <th>description</th>
                <th>price</th>
                <th>thumbnail</th>
                <th>code</th>
                <th>stock</th>
                <th>category</th>
            </tr>
        </thead>
        <tbody>
            {{#each productos}}
            <tr>
                <td>{{this.id}}</td>
                <td>{{this.title}}</td> 
                <td>{{this.description}}</td>
                <td>{{this.price}}</td>
                <td>{{this.thumbnail}}</td>
                <td>{{this.code}}</td>
                <td>{{this.stock}}</td>
                <td>{{this.category}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>